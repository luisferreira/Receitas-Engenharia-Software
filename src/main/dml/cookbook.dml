package pt.ulht.es.cookbook.domain;

class CookBookManager;

class Recipe {
	String recipeTitle;
	String recipeProblemDescription;
	String recipeSolutionDescription;
	String recipeAuthor;
	String tags;
	DateTime creationDate;
}

relation CookBookManagerHasTodoLists {
	CookBookManager playsRole cookbookManager { multiplicity 1..1; }
	Recipe playsRole recipe { multiplicity 0..*; }	
}